{\n                            setRating(rate)\n                        }}\n                        ratingValue={rating}\n                        size={25}\n                        label\n                        transition\n                        fillColor=\"orange\"\n                        emptyColor=\"gray\"\n                    />\n                    <OverlayTrigger\n                        trigger=\"hover\"\n                        placement=\"right\"\n                        overlay={popover}\n                    >\n                        <p className=\"ratingPriority\">?</p>\n                    </OverlayTrigger>\n                </div>\n                {req ? (\n                    <Button\n                        className=\"btn mainColor w-50\"\n                        onClick={handleAddExpenses}\n                    >\n                        Add List\n                    </Button>\n                ) : (\n                    <Button\n                        className=\"w-50 mainColor\"\n                        variant=\"primary\"\n                        disabled\n                    >\n                        <Spinner\n                            as=\"span\"\n                            animation=\"border\"\n                            size=\"sm\"\n                            role=\"status\"\n                            aria-hidden=\"true\"\n                        />\n                        <span className=\"sr-only\">Loading...</span>\n                    </Button>\n                )}\n            </div>\n\n            <div className=\"listt \">\n                <br />\n                <div\n                    className=\"ag-theme-alpine\"\n                    style={{ height: 300, width: 'auto' }}\n                >\n                    <AgGridReact rowData={rowData} >\n                        <AgGridColumn field=\"group\"></AgGridColumn>\n                        <AgGridColumn field=\"name\"></AgGridColumn>\n                        <AgGridColumn 